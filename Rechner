num1 = float(input("Enter a number: "))
operator = input("Choose operator  + - x : ")
num2 = float(input("Enter another number: "))

match operator:
    case "+":
        print(num1 + num2)
    case "-":  
        print(num1 - num2)
    case "x":
        print(num1 * num2)
    case ":":
        print(num1 / num2)
    case _:
        print("Incorrect operator")    